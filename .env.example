# CONFIGURA√á√ïES DO ETL
ETL_LOAD_STRATEGY=replace
ETL_QUERY_TIMEOUT=300
ETL_BATCH_SIZE=1000
ETL_LOG_LEVEL=INFO

# DIRET√ìRIOS
SQL_SCRIPTS_PATH=/opt/etl_teste_geo/sql_scripts
