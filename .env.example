POSTGRES_SOURCE_PORT=5432
POSTGRES_SOURCE_DATABASE=origem_db
POSTGRES_SOURCE_USER=postgres
POSTGRES_SOURCE_PASSWORD=sua_senha_postgresql_aqui

# CONFIGURA√á√ïES MYSQL (DESTINO)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=destino_db
